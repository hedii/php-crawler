{
    "name": "hedii/php-crawler",
    "description": "A crawler application written with php and Laravel that finds email addresses on the internets.",
    "keywords": ["php-crawler", "hedii", "crawler", "php", "laravel", "email"],
    "homepage": "https://github.com/hedii/php-crawler",
    "authors": [
        {
            "name": "Hedi Chaibi",
            "email": "contact@hedichaibi.com",
            "homepage": "https://hedichaibi.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "doctrine/dbal": "^2.6",
        "fideloper/proxy": "~4.0",
        "guzzlehttp/guzzle": "^6.3",
        "hedii/extractors": "^2.0",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "~1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "nunomaduro/collision": "~1.1",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
